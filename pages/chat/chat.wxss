.close_btn {
  width: 44rpx;
  height: 44rpx;
  border: solid 2rpx #999;
  border-radius: 50%;
  position: relative;
}

.close_btn::before, .close_btn::after {
  width: 24rpx;
  height: 2rpx;
  background: #999;
  content: '';
  position: absolute;
  left: 50%;
  top: 50%;
  margin: -1rpx 0 0 -12rpx;
}

.close_btn::before {
  transform: rotate(45deg);
}

.close_btn::after {
  transform: rotate(-45deg);
}


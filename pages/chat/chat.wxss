.checkbox {
  display: inline-block;
  position: absolute;
  right: 44rpx;
  bottom: 44rpx;
}

.checkbox::after, .checkbox::before {
  content: '';
  display: inline-block;
  position: absolute;
  box-sizing: border-box;
  top: 50%;
  transform: translate3d(0, -50%, 0);
}

.checkbox::before {
  width: 44rpx;
  height: 44rpx;
  border-radius: 50%;
  border: 2rpx solid #ccc;
  background: #fff;
  left: 0;
}

.checkbox.rd_sel::before {
  background: #ff0036;
  border-color: #ff0036;
}

.checkbox.rd_sel::after {
  top: -13rpx;
  left: 16rpx;
  right: 20rpx;
  width: 12rpx;
  height: 25rpx;
  border-width: 0 4rpx 4rpx 0;
  border-color: #fff;
  border-style: solid;
  -webkit-transform: rotate(45deg);
  transform: rotate(45deg);
}


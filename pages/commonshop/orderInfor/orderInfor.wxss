}

.bg_bar {
  width: 100%;
  height: 4rpx;
  background-image: repeating-linear-gradient(45deg, #ff6c71 0, #ff6c71 25rpx, white 25rpx, white 30rpx, #2885fc 30rpx, #2885fc 55rpx, white 55rpx, white 60rpx);
}

/*swtich样式-start*/

/*swtich整体大小*/

.wx-switch-input {
  width: 82rpx !important;
  height: 50rpx !important;
}

/*白色样式（false的样式）*/

.wx-switch-input::before {
  width: 80rpx !important;
  height: 46rpx !important;
}

/*红色样式（true的样式）*/

.wx-switch-input::after {
  width: 40rpx !important;
  height: 46rpx !important;
}

/*swtich样式end*/


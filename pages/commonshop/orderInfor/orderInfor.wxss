/* 优惠券弹窗 */
.cmask {
  width: 100%;
  height: 100%;
  bottom: 0;
  left: 0;
  position: fixed;
  background: rgba(0, 0, 0, 0.5);
  transition: all 0.3s ease-in-out;
  z-index: 99;
}
.cm_bot_box {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  box-sizing: border-box;
  background: #fff;
  transition: all 0.3s ease-in-out 0.08s;
}
.cpp_close {
  display: inline-block;
  position: absolute;
  top: 15rpx;
  right: 20rpx;
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
}
.cpp_close_img{
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  margin: -24rpx 0 0 -24rpx;
}
.cmask .zengpinMain{
  margin-top: 80rpx;
  max-height: 840rpx;
  padding: 10rpx 30rpx;
  background: #f2f2f2;
  box-sizing: border-box;
}
.cmask .zengpin{
  margin-bottom: 20rpx;
  background: #fff;
  border-bottom: 2rpx solid #eaeaea;
}
.cmask .zengpin:last-child{
  border-bottom: none;
}

.cpn_chk{
  display:flex;
  flex-direction:row;
  justify-content:center;
  align-items:center;
  width:44rpx;
  height:44rpx;
  border-radius:50%;
  border:2rpx solid #ccc;
  background:#fff;
  position:relative;
  flex-shrink:0;
}
.cpn_chk:before{
  content:'';
  display:block;
  width:100rpx;
  height:100rpx;
  position:absolute;
  top:50%;
  left:50%;
  margin:-50rpx 0 0 -50rpx;
}
.cpn_cur{
  border-color:#f03;
  background:#f03;
}
.cpn_gray {
  border-color:#eaeaea;
  background: #eaeaea;
}
.cpn_gray::after, .cpn_cur:after{
  content:'';
  display:inline-block;
  width:20rpx;
  height:10rpx;
  border:4rpx solid #FFF;
  border-width:0 0 4rpx 4rpx;
  position:relative;
  transform:rotate(-45deg);
  margin-top:-8rpx;
}

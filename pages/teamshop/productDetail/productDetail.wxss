  word-break: break-all;
}

.pd_i_cur {
  border-color: #ff4949;
}

.pd_i_cur:before, .pd_i_cur:after {
  content: '';
  display: inline-block;
  position: absolute;
}

.pd_i_cur:before {
  width: 0;
  height: 0;
  border-width: 10rpx;
  border-style: solid;
  border-top-left-radius: 6rpx;
  border-color: #ff4949 transparent transparent #ff4949;
  top: -1rpx;
  left: -1rpx;
}

.pd_i_cur:after {
  width: 8rpx;
  height: 4rpx;
  border: 4rpx solid #fff;
  border-width: 0 0 3rpx 3rpx;
  position: absolute;
  top: 0;
  left: 0;
  transform: rotate(-45deg);
}


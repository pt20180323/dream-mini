/*弹窗*/
.pop_prop_box{
  position:fixed;
  width:100%;
  z-index: 99;
  bottom:0;
  left:0;  
}
.mask{
  width:100%;
  height:100%;
  bottom:0;
  left:0; 
  position:fixed;
  background:rgba(0,0,0,0.5);
  transition:all 0.3s ease-in-out;
  display:none;
}
.m_slideUp .mask{
  display:block;
}
.m_bot_box{
  position:absolute;
  left:0;
  bottom:0;
  width:100%;
  box-sizing:border-box;
  background:#FFF;
  transition:all 0.3s ease-in-out 0.08s;
  transform:translate3d(0,110%,0);
}
.m_slideUp .m_bot_box{
  transform:translate3d(0,0,0);
}

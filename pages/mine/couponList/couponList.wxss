page{
  background:#f2f2f2;
}
.list-g-box{
  position:absolute;
  top:100rpx;
  bottom:0;
  left:0;
  right:0;
}
.o_nav_i{
  display:flex;
  flex-direction:row;
  flex:1;
  height:100rpx;
  justify-content:center;
  align-items:center;
  position:relative;
  color:#000;
  background:#FFF;
}
.o_nav_c{
  color:#ff4949;
}
.o_nav_c:after{
  content:'';
  display:block;
  position:absolute;
  left:25rpx;
  right:25rpx;
  bottom:0;
  height:5rpx;
  background:#ff4949;
}
.couponsList{
  padding: 0 25rpx 25rpx 25rpx;
  overflow:hidden;
}
.couponsBox{
  margin-top:25rpx;
  overflow:hidden;
  border-radius:15rpx;
  display: inline-block;
  width:700rpx
}
.couponsTop {
  height:54rpx;
  line-height:54rpx;
  color:#fff;
  padding:3rpx 22rpx 4rpx 22rpx;
}
.wait .couponsTop, .used  .couponsTop {
  background:#ff4949;
}
.overdue .couponsTop{
  background:#9a9a9a;
}
.couponsBottom{
  background:#fff;
  min-height:122rpx;
  position:relative;
  padding:16rpx 30rpx 22rpx 228rpx;
}
.couponsBottom image{
  position:absolute;
  top:-4rpx;
  left:0;
  width:100%;
  height:6rpx;
}
.wait .couponsBottom::after{
   content:'';
  display:inline-block;
  width:12rpx;
  height:12rpx;
  position:absolute;
  top:50%;
  right:18rpx;
  border:2rpx solid #999;
  border-width:0 2rpx 2rpx 0;
  transform:rotate(-45deg);
  margin-top:-8rpx;
}
.used .couponsBottom::after,.overdue .couponsBottom::after {
  width:167rpx;
  height:144rpx;
  content:'';
  position:absolute;
  top:0;
  right:18rpx;
}
.used .couponsBottom::after {
  background-image:url(data:image/png;base64,);
  background-size:100% 100%;
}
.overdue .couponsBottom::after {
  background: url(data:image/png;base64,);
  background-size:100% 100%;
}
.fontFamilyArial{
  font-family: Arial
}
.extras{
  position:absolute;
  left:20rpx;
  top:16rpx;
  height:122rpx;
  width:144rpx;
  border-right:2rpx solid #ccc;
  color:#333;
  padding:0 10rpx 0 20rpx;
  line-height:122rpx;
  font-size:68rpx;
  font-family:"Arial";
  overflow:hidden;
  text-align:center;
}
.extras::before{
  content:'￥';
  position:absolute;
  left:0;
  top:50%;
  height:30rpx;
  line-height:30rpx;
  font-size:30rpx;
  margin-top:-5rpx;
}
.couponsName{
  display:inline-block;
  width:100%;
  overflow:hidden;
  height:46rpx;
  color:#333;
  line-height:46rpx;
  font-size:30rpx;
  margin-top:14rpx;
}
.couponsTerm{
  display:inline-block;
  width:100%;
  height:38rpx;
  line-height:38rpx;
  margin-top:4rpx;
  color:#666;
  font-size:24rpx;
}
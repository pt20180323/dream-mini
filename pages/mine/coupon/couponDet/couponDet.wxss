.page {
  display: block;
  min-height: 100%;
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
  background: #fff;
  border-top: 1rpx solid #f5f5f5;
  box-sizing: border-box;
}
.content{
  padding: 0 25rpx;
}
.content>.con_common:nth-child(1){
  margin-top: 30rpx;
}
.con_common{
  position: relative;
  height: 80rpx;
}
.txt{
  display: inline-block;
  color: #737373;
  padding-right: 50rpx;
  width: 24%;
  box-sizing: border-box;
}
.desc{
  position: absolute;
  display: inline-block;
  color: #000;
  width: 76%;
}
.phone{
  color: #309430;
}